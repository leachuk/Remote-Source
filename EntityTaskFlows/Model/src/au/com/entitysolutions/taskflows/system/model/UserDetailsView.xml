  <SQLQuery>
    <![CDATA[SELECT SysUsers.USER_ID,
       SysUsers.USER_NAME,
       SysUsers.DESCRIPTION,
       SysUsers.PASSWORD,
       --SysUsers.PERSON_ID,
       SysUsers.CUSTOMER_ID,
       SysUsers.SUPPLIER,
       SysUsers.EMAIL,
       SysUsers.FROM_DATE,
       SysUsers.TO_DATE,
       SysUsers.CREATED_BY,
       SysUsers.CREATION_DATE,
       SysUsers.LAST_UPDATED_BY,
       SysUsers.LAST_UPDATE_DATE,
       SysUsers.SECURITY_GROUP,
       SysUsers.SECURITY_PATTERN,
       SysUsers.LAST_LOGIN,
       SysUsers.LAST_PASSWORD_CHANGE,
       SysUsers.LOCKED,
       SysUsers.LOGIN_ATTEMPTS,
       SysUsers.PROP_TO_PORTAL,
       SysUsers.PIN,
       SysUsers.DISABLED_DATE,
       SysUsers.SECURITY_QUESTION_1,
       SysUsers.SECURITY_QUESTION_ANSWER_1,
       SysUsers.SECURITY_QUESTION_2,
       SysUsers.SECURITY_QUESTION_ANSWER_2,
       SysUsers.PORTAL_SKIN,
       SysUsers.PORTAL_SECURITY_ROLE,
       SysUsers.RESET_PASSWORD,
       SysUsers.RESET_TERM_CONDITION,
       SysUsers.IS_PORTAL_USER,
       Person.FIRST_NAME,
       Person.SURNAME,
       Person.PREFERRED_NAME,
        SysUsers.PRIMARY_CONTACT,              
       SysUsers.SECONDARY_CONTACT,
       PERSON.ECMS_CS_CONSULTANT_ID,
       PERSON.PERSON_ID,
       nvl(ecms_cons.email_work, ecms_cons.email_home) ecms_cons_contact_email,
       PERSON.CUSTOMER_SUPPORT,
       nvl(cust_support.email_work, cust_support.email_home) cust_support_contact_email,
SysUsers.IS_ACTIVE,
(select pop.ORG_ID FROM portal_org_profile pop where SysUsers.PORTAL_ORG_PROFILE_ID = pop.PORTAL_ORG_PROFILE_ID) as org_id, 
HP_REPORTS_PKG.get_organisation_name((select pop.ORG_ID FROM portal_org_profile pop where SysUsers.PORTAL_ORG_PROFILE_ID = pop.PORTAL_ORG_PROFILE_ID),NULL) business_group_name,
(select ecms_cons_person.FIRST_NAME || ', ' || ecms_cons_person.SURNAME FROM  HP_PERSON_D ecms_cons_person where ecms_cons_person.PERSON_ID=PERSON.ECMS_CS_CONSULTANT_ID) ecms_cons_fullname,
(select cust_support_person.FIRST_NAME || ', ' || cust_support_person.SURNAME FROM  HP_PERSON_D cust_support_person where cust_support_person.PERSON_ID=PERSON.CUSTOMER_SUPPORT) cust_support_fullname
FROM SYS_USERS SysUsers, HP_PERSON_D Person, HP_ADDRESS_CONTACT_D ecms_cons
, HP_ADDRESS_CONTACT_D cust_support
WHERE SysUsers.PERSON_ID = Person.PERSON_ID
AND  ecms_cons.person_id(+) = PERSON.ECMS_CS_CONSULTANT_ID
AND  cust_support.person_id(+) = PERSON.CUSTOMER_SUPPORT
AND SysUsers.IS_ACTIVE = 'Y']]>
